{
  "name": "@sourcegraph/lsif-python",
  "version": "0.1.0",
  "description": "LSIF indexer for Python",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode production --progress",
    "clean": "shx rm -rf ./dist ./out README.md LICENSE.txt",
    "prepack": "npm run clean && shx cp ../../README.md . && shx cp ../../LICENSE.txt . && npm run build",
    "check-snapshots": "node ./index.js snapshot-dir snapshots --envCacheFile snapshots/testEnv.json --diff",
    "update-snapshots": "node ./index.js snapshot-dir snapshots --envCacheFile snapshots/testEnv.json",
    "webpack": "webpack --mode development --progress",
    "watch": "webpack --mode development --progress --watch"
  },
  "author": "Sourcegraph",
  "repository": {
      "type": "git",
      "url": "https://github.com/sourcegraph/lsif-python",
      "directory": "packages/pyright-lsif/"
  },
  "license": "MIT",
  "devDependencies": {
    "@sourcegraph/eslint-config": "0.26.0",
    "@sourcegraph/prettierrc": "3.0.3",
    "@sourcegraph/tsconfig": "4.0.1",
    "@types/diff": "^5.0.2",
    "@types/glob": "^7.2.0",
    "@types/google-protobuf": "^3.15.5",
    "@types/yargs": "^17.0.9",
    "copy-webpack-plugin": "^10.2.0",
    "shx": "^0.3.3",
    "ts-loader": "^9.2.6",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.5.4",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "diff": "^5.0.0",
    "glob": "^7.2.0",
    "google-protobuf": "^3.19.3",
    "ts-node": "^10.5.0",
    "vscode-languageserver": "^7.0.0",
    "yargs": "^17.3.1"
  },
  "files": [
    "/dist",
    "LICENSE.txt"
  ],
  "bin": {
    "lsif-python": "index.js"
  }
}
